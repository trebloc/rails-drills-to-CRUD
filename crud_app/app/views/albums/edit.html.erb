<h1>Edit Album</h1>
<%= form_for :album, url: patch_album_path(:id => @album.id), method: "patch" do |f| %> 
  <p>
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </p>
 
  <p>
    <%= f.label :album %><br>
    <%= f.text_field :album %>
  </p>

  <p>
    <%= f.label :year %><br>
    <%= f.text_field :year %>
  </p>    
 
  <p>
    <%= f.label :cover_art %><br>
    <%= f.text_field :cover_art %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
