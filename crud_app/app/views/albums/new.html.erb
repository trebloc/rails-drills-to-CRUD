<h1>New Album</h1>
<%= form_for @album do |f| %>
  <p>
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </p>
 
  <p>
    <%= f.label :album %><br>
    <%= f.text_field :album %>
  </p>

  <p>
    <%= f.label :cover_art %><br>
    <%= f.text_field :cover_art %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>